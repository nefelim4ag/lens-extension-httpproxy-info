{
  "name": "lens-httpproxy-info",
  "version": "1.1.0",
  "author": "Timofey Titovets <nefelim4ag@gmail.com>",
  "maintainers": [],
  "description": "Contour HTTPProxy Info",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nefelim4ag/lens-extension-httpproxy-info.git"
  },
  "engines": {
    "lens": "^6.0.0"
  },
  "renderer": "dist/renderer.js",
  "lens": {
    "metadata": {},
    "styles": []
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "dev": "npm run build --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@k8slens/extensions": "^6.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^20.11.20",
    "@types/react": "^18.2.28",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "prettier": "3.0.3",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.5",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "homepage": "https://github.com/nefelim4ag/lens-extension-httpproxy-info",
  "bugs": {
    "url": "https://github.com/nefelim4ag/lens-extension-httpproxy-info/issues"
  },
  "keywords": [
    "extension",
    "k8slens",
    "lens"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "*.tgz",
              "name": "lens-httpproxy-info.tgz"
            }
          ]
        }
      ],
      "@semantic-release/git"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "files": [
    "dist/*.js"
  ]
}
